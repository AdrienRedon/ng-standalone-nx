{
  "projectType": "library",
  "sourceRoot": "packages/pipes/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/pipes"],
      "options": {
        "jestConfig": "packages/pipes/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/pipes/**/*.ts",
          "packages/pipes/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
